list=['apple','banana','cherry','litchi','grapes']
print(list)

list.append('guava')
print(list)


newlist=list.copy()
print(newlist)

count=list.count('cherry')
print(count)


ind=list.index('litchi')
print(ind)


list.insert(2,'orange')
print(list)

list.pop(2)
print(list)

list.remove('cherry')
print(list)


list.reverse()
print(list)

list.sort()
print(list)
